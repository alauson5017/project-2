<h1>Add New Action Figure</h1>

<form action="/figures" method="POST">
  <div class="form-group">
    <label for="name">Name</label>
    <input type="text" class="form-control" id="name" name="name" />
    <label for="year">Year</label>
    <input type="text" class="form-control" id="year" name="year" />
    <label for="condition">Condition</label>
    <input type="text" class="form-control" id="condition" name="condition" />
    <label for="toyLine">Toy Line</label>
    <input type="text" class="form-control" id="toyLine" name="toyLine" />
    <label for="hasPackaging">Has Packaging?</label>
    <input type="checkbox" name="hasPackaging" id="hasPackaging" /><br />
    <!-- <label for="doWant">Do I want it?</label>
    <input type="checkbox" name="doWant" id="doWant" /><br /> -->
    <label for="doHave">Do I have it?</label>
    <input type="checkbox" name="doHave" id="doHave" /><br />
  



</div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>